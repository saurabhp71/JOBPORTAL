@model JobPortalLibrary.Admin.AdminUser

@{
    ViewBag.Title = "siteEarningS";
    Layout = "~/Views/Shared/Admin_Layout.cshtml";
}
&nbsp;&nbsp;
<div class="content-body">

    <div class="container-fluid mt-3">
        <div class="row">
            <div class="col-lg-3 col-sm-6">
                <div class="card gradient-1" style="height:170px">
                    <div class="card-body">
                        <h3 class="card-title text-white">Total Earning</h3>
                        <br />
                        <div class="d-inline-block">
                            <h2 class="text-white"><p>@ViewBag.TotalAmount</p></h2>
                            <p class="text-white mb-0"></p>
                        </div>
                        <span class="float-right display-5 opacity-5"><i class="fa fa fa-money"></i></span>
                    </div>
                </div>
            </div>
            <div class="col-lg-3 col-sm-6">
                <div class="card gradient-2" style="height:170px">
                    <div class="card-body">
                        <h3 class="card-title text-white"> Total Month Earning</h3>
                        <br />
                        <div class="d-inline-block">
                            <h2 class="text-white"><p>@ViewBag.Currentmonth </p></h2>
                            <p class="text-white mb-0"></p>
                        </div>
                        <span class="float-right display-5 opacity-5"><i class="fa fa fa-money"></i></span>
                    </div>
                </div>
            </div>
        </div>
    </div>



