    <table id="example" class="table table-bordered table-striped mt-4 dt-responsive nowrap table-responsive" style="margin-left: 80px; width: 900px; border: solid; box-shadow: 0 4px 8px 0 rgba(0,0,0,0.2),0 6px 20px 0 rgba(0,0,0,0.19); overflow: scroll;">
        <thead>
            <tr style="background-color:lavender; color: black;">
                @*<th>
                        Selectall<br />
                        <input type="checkbox" id="selectall" class="css-checkbox " name="selectall" />

                    </th>*@

                <th>
                    @Html.DisplayNameFor(model => model.Seekercode)
                </th>
                @*<th>
                        @Html.DisplayNameFor(model => model.CompanyName)
                    </th>*@

                <th style="width:500px;">
                    @Html.DisplayNameFor(model => model.SubscriptionName)

                </th>

                <th>
                    @Html.DisplayNameFor(model => model.SubscriptionDuration)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Offer)
                </th>

                <th>
                    @Html.DisplayNameFor(model => model.PlanPricing)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.OfferedPrice)
                </th>

            </tr>
        </thead>

        <tbody>
            @foreach (var item in Model.User)
            {
                <tr>
                    @*<td class="checkbox">
                            @Html.CheckBox("multiSelect", new { id = "multiselect", @class = "checkbox", Value = item.AppliedJobId })
                        </td>*@

                    <td>
                        @Html.DisplayFor(modelItem => item.Seekercode)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.SubscriptionName)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.SubscriptionDuration)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Offer)
                    </td>

                    <td>
                        @Html.DisplayFor(modelItem => item.PlanPricing)

                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.OfferedPrice)
                    </td>
                </tr>
            }
        </tbody>

    </table>
